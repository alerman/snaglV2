@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="homeHeader">
    <img src="/Content/Image/Logo.png" alt="SnagL Logo" title="SnagL" />
</div>
<table id="projectList">
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
        </tr>
    </thead>
    <tr class="gvRow">
        <td>@Html.ActionLink("Live Data", "Index", "LiveData")</td>
        <td>Demonstrates the live data functionality</td>
    </tr>
    <tr class="gvRow alternate">
        <td>@Html.ActionLink("3 Wheel", "Index", "ThreeWheel")</td>
        <td>Graph with 3 Wheels</td>
    </tr>
    <tr class="gvRow">
        <td>@Html.ActionLink("External Resources", "Index", "ExternalResource")</td>
        <td>Demonstrates the ability to load external resources</td>
    </tr>
    <tr class="gvRow alternate">
        <td>@Html.ActionLink("Web API", "Index", "WebApiDemo")</td>
        <td>Demonstrates the SnagL&trade; Web API</td>
    </tr>
    <tr class="gvRow">
        <td>@Html.ActionLink("Geospatial Data", "Index", "Geospatial")</td>
        <td>Demonstrates GraphML containing geospatial data</td>
    </tr>
</table>
