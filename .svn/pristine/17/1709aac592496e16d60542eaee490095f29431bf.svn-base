<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/WebApiDemo.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/ui-darkness/jquery-ui-1.8.11.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.6.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Silverlight.js")" type="text/javascript"></script>
</head>
<body>
    <div id="header">
        <a href="/"><img src="/Content/Image/Home.gif" alt="Home" title="Home" /></a>
    </div>
    @RenderBody()
    <div id="footer">
        <img src="/Content/Image/LogoSmall.png" alt="SnagL" /> <label>SnagL&trade; - Copyright&copy; @DateTime.Now.Year</label>
    </div>
</body>
</html>
