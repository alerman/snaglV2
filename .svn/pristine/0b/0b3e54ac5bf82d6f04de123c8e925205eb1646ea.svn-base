using System.Xml.Serialization;

namespace SnagL.FacebookHostDemo.GraphML
{
    /// <summary>
    /// Specifies the edge type
    /// </summary>
    public enum EdgeType
    {
        /// <summary>
        /// Indicates a directed edge
        /// </summary>
        [XmlEnum("directed")]
        Directed,

        /// <summary>
        /// Indicates an undirected edge
        /// </summary>
        [XmlEnum("undirected")]
        Undirected
    }
}
